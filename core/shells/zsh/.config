;; -*- mode: clojure -*-
(
 (:packages (:apt "zsh")
            (:pacman "zsh")
            (:msys "zsh"))

 #dot/link-gen
 (:link ("$XDG_CONFIG_HOME/zshell"
         (".zshrc" ".zshenv"))
        ;; As much as it pains me but because zshell doesn't source
        ;; .profile there's no where else I can define zdotdir outside
        ;; of home without messing with /etc/zprofile or other locations.
        "~/.zshenv")

 (:link
  "bindings" "$XDG_CONFIG_HOME/bindings/zshell"
  "prompt" "$XDG_CONFIG_HOME/prompt/zsh")
)
