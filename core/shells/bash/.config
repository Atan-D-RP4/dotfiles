;; -*- mode: clojure -*-
(
 (:link
  "bindings" "$XDG_CONFIG_HOME/bindings/bash"
  {:force true :src "bashrc" :dest "~/.bashrc"}
  {:force true :src "logout" :dest "~/.bash_logout"}
  {:force true :src "profile" :dest "~/.bash_profile"}
  {:force true :src "prompt" :dest "$XDG_CONFIG_HOME/prompt/bash"})
)
