;; -*- mode: clojure -*-
(
 (:clean {:path "$XDG_CONFIG_HOME/autoloads" :recursive true})

 #dot/link-gen
 (:link ("$XDG_CONFIG_HOME/autoloads/"
          ( "global" "bash" "zshell"
           "linux" "macos" "windows"))
