;; -*- mode: clojure -*-
(
 (:clean {:path "$XDG_CONFIG_HOME/autoloads" :recursive true})

 #dot/link-gen
 (:link ("$XDG_CONFIG_HOME/autoloads/"
         ("global" "linux" "macos" "windows"))
        {:src "cmds/*" :dest "$XDG_CONFIG_HOME/autoloads/cmds/" :glob true}
        {:src "shell/*" :dest "$XDG_CONFIG_HOME/autoloads/shell/" :glob true})
)
