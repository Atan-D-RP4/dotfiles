# -*- mode: sh -*-
fzbm() {
  ls-bookmarks -p '' |
    sed '/^javascript:/d' |
    FZF_DEFAULT_OPTS="$FZF_SHELL_OPTS" fzf -d $'\t' --ansi --with-nth '2..' --multi \
                    --bind 'alt-return:execute-silent(xopen {1} &)+down' \
                    --bind 'ctrl-y:execute-silent(echo {1} | clip)' |
    cut -d $'\t' -f 1 |
    xargs -d '\n' -r xopen
}
