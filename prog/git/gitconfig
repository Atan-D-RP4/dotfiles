[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	autocrlf = input
[alias]
	c = commit
	s = status
	a = add
	b = branch
	cl = clone
	d = diff
	l = log
	v = checkout
	m = mv
	x = rm
	r = rebase
	p = push
	f = pull
	unstage = reset HEAD --
	fzlog = !git_fzlog
	g = fzlog
    undo = reset HEAD~
