;; -*- mode: clojure -*-
(
 (:packages (:choco "dropbox")
            (:yay "dropbox"))

 (:link {:if-bots "emacs"
         :src "~/Dropbox/org"
         :dest "../editors/emacs/org"
         :ignore-missing true}
        {:ignore-missing true :src "~/Dropbox/org/docs"  :dest "$XDG_DOCUMENTS_DIR/pdfs"}
        {:ignore-missing true :src "~/Dropbox/org/brain" :dest "$XDG_DOCUMENTS_DIR/brain"}
        {:ignore-missing true :src "~/Dropbox/org/notes" :dest "$XDG_DOCUMENTS_DIR/notes"}
        {:ignore-missing true :src "~/Dropbox/org/bib"   :dest "$XDG_DOCUMENTS_DIR/bibliography"})
)
