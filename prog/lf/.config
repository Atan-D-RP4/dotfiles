;; -*- mode: clojure -*-
(
 (:packages (:yay "lf")
            (:choco "lf")
            (:go "github.com/gokcehan/lf"))

 #dot/link-gen
 (:link "~/.config/lf/lfrc"
        {:src "./auto/*" :dest "~/.config/autoloads/cmds/" :glob true}
        {:src "./cmds/*" :dest "~/.config/lf/cmds/" :glob true}
        {:src "dolphin"
         :dest "~/.local/share/kservices5/ServiceMenus/"
         :if-bots "dolphin"})

 #dot/if-windows #dot/link-gen
 (:link "~/AppData/Local/lf/lfrc")
)
